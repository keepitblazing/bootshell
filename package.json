{
  "name": "bootshell",
  "displayName": "Bootshell Claude Launcher",
  "description": "Launch Claude with location prompt",
  "publisher": "kylepark",
  "version": "0.1.14",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:bootshell.openClaudeTerminal"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bootshell.openClaudeTerminal",
        "title": "Open Claude Terminal"
      }
    ]
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/keepitblazing/bootshell"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.0",
    "vsce": "^2.20.0"
  }
}
